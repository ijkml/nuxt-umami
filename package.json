{
  "name": "nuxt-umami",
  "type": "module",
  "version": "2.1.4",
  "description": "Integrate Umami Analytics into Nuxt",
  "author": "ML <me.mlaure@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ijkml/nuxt-umami/tree/next",
  "repository": "https://github.com/ijkml/nuxt-umami/tree/next",
  "keywords": [
    "nuxt",
    "umami",
    "analytics",
    "tracking",
    "nuxt-umami",
    "nuxt layer",
    "nuxt module",
    "nuxt plugin"
  ],
  "main": "./nuxt.config.ts",
  "files": [
    "utils/",
    "middleware/",
    "internal/",
    "app.config.ts",
    "app.vue",
    "nuxt.config.ts"
  ],
  "scripts": {
    "dev": "nuxi prepare & nuxi dev .playground",
    "build": "nuxi build .playground",
    "generate": "nuxi generate .playground",
