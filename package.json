{
  "name": "nuxt-umami",
  "type": "module",
  "version": "3.0.0-beta.4",
  "description": "Integrate Umami Analytics into Nuxt",
  "author": "Moses Laurence <me@ijkml.dev>",
  "license": "MIT",
  "homepage": "https://umami.nuxt.dev/",
  "sponsor": {
    "url": "https://github.com/sponsors/ijkml/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ijkml/nuxt-umami.git"
  },
  "packageManager": "pnpm@9.9.0",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "up": "npm run dev:prepare && npm run dev",
    "release": "npm run lint && npm run test && npm run prepack && bumpp && npm publish --tag module && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.13.1",
    "request-ip": "^3.3.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.5.6",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.13.1",
    "@types/node": "^22.5.4",
    "@types/request-ip": "^0.0.41",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "nuxt": "^3.13.1",
    "typescript": "latest",
    "vue-tsc": "^2.1.6"
  }
}
