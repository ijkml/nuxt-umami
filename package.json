    "release:beta": "bumpp prerelease --commit --push --no-tag && npm publish --access public --tag beta"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
